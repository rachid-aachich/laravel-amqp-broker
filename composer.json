{
    "name": "maroeco/message-broker",
    "description": "Message Broker Library for MaroEco Laravel",
    "require": {
        "php": "^7.4|^8.0|^10.0",
        "nesbot/carbon": "^2.53",
        "illuminate/support": "^9.0",
        "php-amqplib/php-amqplib": "^3.5",
        "spatie/async": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "MaroEco\\MessageBroker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MaroEco\\MessageBroker\\MessageBrokerServiceProvider"
            ]
        }
    }    
}
