{
    "name": "aachich/message-broker",
    "description": "AMQP Message Broker abstraction for Laravel - easily switch between RabbitMQ and other AMQP-compatible brokers",
    "keywords": ["laravel", "rabbitmq", "amqp", "message-broker", "queue", "messaging", "php"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Aachich",
            "email": "info@aachich.com"
        }
    ],
    "homepage": "https://github.com/rachid-aachich/laravel-amqp-broker",
    "support": {
        "issues": "https://github.com/rachid-aachich/laravel-amqp-broker/issues",
        "source": "https://github.com/rachid-aachich/laravel-amqp-broker"
    },
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^2.53|^3.0",
        "illuminate/support": "^10.0|^11.0",
        "php-amqplib/php-amqplib": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Aachich\\MessageBroker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aachich\\MessageBroker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aachich\\MessageBroker\\MessageBrokerServiceProvider"
            ],
            "aliases": {
                "MessageBroker": "Aachich\\MessageBroker\\Facades\\MessageBroker"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
